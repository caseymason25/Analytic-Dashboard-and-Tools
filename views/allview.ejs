<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en" xmlns:v="urn:schemas-microsoft-com:vml">
<head>
  <title>All Team View</title>
  <link rel="stylesheet" href="../css/allview.css">
  <link rel="stylesheet" href="../css/select2.css">
  <link rel="stylesheet" href="../css/bootstrap.css">
  <link rel="stylesheet" href="../css/bootstrap-dialog.min.css">
  <link rel="stylesheet" href="../css/font-awesome.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
  <script src="/lib/jquery.tablesorter.js"></script>
  <script src="/lib/bootstrap.min.js"></script>
  <script src="/lib/bootstrap-dialog.min.js"></script>
  <script src="/lib/select2.js"></script>
  <script src="/lib/flot/jquery.flot.js"></script>
  <script src="/lib/flot/jquery.flot.resize.min.js"></script>
  <script src="/lib/flot/jquery.flot.sparkline.js"></script>
  <script src="/lib/flot/jquery.flot.time.min.js"></script>
  <script src="../lib/moment.min.js"></script>
  <script src="../lib/moment-timezone.min.js"></script>
  <script src="/js/allview.js" type="text/javascript"></script>
  <script src="/js/dialogs.js" type="text/javascript"></script>
  <script>
  page_version = '<%= page_version %>';
  </script>
</head>
<body>
  <div id='wrapper_allview'>
    <span id='server-status'></span>
    <div onclick='javascript:change_view_dialog();' data-toggle="modal" class="configBTN">
      <i class="fa fa-gears">
      </i>
    </div>
    <img src='/img/allview_loading.gif' title='loading allview'/>
    <div id='choose_view' class="hidden">
      <div class='modal-body' role='dialog' aria-labelledby='selectTeam' aria-hidden='false'>
        <select id='viewlist' style='width:98%' tabindex='-1' class="basic-select">
          <%
          var selected = " ";
          for(var z=0;z<items.length;z++) {
            if(items[z].mnemonic === item.mnemonic) {
              selected = "selected";
            }
            if(items[z].public === 1 && items[z].active === 1 && items[z].allview_ind === 1) {
              %>
              <option value='<%= items[z].mnemonic %>'<%= selected %> ><%= items[z].mnemonic %></option>
              <%
            }
            selected = " ";
          }
          %>
        </select>
        <span class='contact'>Issue or request, please <a onclick='javascript:show_contact();'>contact us</a></span>
        <span class='contact'><a href='/manage'>Create/Edit View</a></span>
        <%
        if(typeof cache_dt_tm !== 'undefined') {
          %>
          <span class='contact'>Cache Built at: <%= cache_dt_tm %></span>
          <%
        }
        if(typeof snapshot_dttm !== 'undefined') {
          %>
          <span class='contact'>Last Data Snapshot: <%= snapshot_dttm %></span>
          <%
        }
        %>
        <span class='contact'><a href='/changelog'>Changelog</a></span>
      </div>
    </div>
  </div>
</body>
</html>
